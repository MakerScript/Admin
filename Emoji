local Emoji = {
    [1699124209] = true;
    [1873453313] = true;
    [1976184447] = true;
    [1758543757] = true;
    [469653868] = true;
}

local function Emoji_System()
    for i,v in pairs(game.Players:GetChildren()) do
        if Emoji[v.UserId] then
            if v.Character then
                if v.Character.Parent.Name == 'Players' then
                    v.Character:FindFirstChildWhichIsA('Humanoid').DisplayName = ('[‚úÖ‚≠ê]'..v.DisplayName)
                end
            end
        else
            if v.Character then
                if not v.Character.LeftUpperLeg:FindFirstChild("OriginalSize") then
                    v.Character:FindFirstChild("Humanoid").DisplayName = "[üò≥]"..v.DisplayName
                end
            end
        end
    end
end
local success, err = pcall(Emoji_System)
return Emoji
